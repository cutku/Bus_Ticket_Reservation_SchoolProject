<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="81dp">

    <ListView
        android:id="@+id/listViewAdminUpdateTrip"
        android:layout_width="0dp"
        android:layout_height="326dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="11dp"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/activityListTripsForAdminUpdateTripButton"
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:onClick="updateTrip"
        android:text="Update Trip"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activityListTripForAdminObjectIdTextUpdateTrip" />

    <EditText
        android:id="@+id/activityListTripForAdminObjectIdTextUpdateTrip"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ems="10"
        android:hint="Write Object Id"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/activityListTripsForAdminUpdateTripButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/listTipsActivityForAdminForUpdateDestinationSpinner"
        app:layout_constraintTop_toBottomOf="@+id/listTripForAdminFroUpdateTripDepartureDateButton" />

    <Spinner
        android:id="@+id/listTipsActivityForAdminForUpdateDestinationSpinner"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="11dp"
        app:layout_constraintBottom_toTopOf="@+id/activityListTripsForAdminUpdateTripButton"
        app:layout_constraintEnd_toStartOf="@+id/activityListTripForAdminObjectIdTextUpdateTrip"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Spinner
        android:id="@+id/listTripsForAdminForUpdateFromSpinner"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/listTripForAdminFroUpdateTripDepartureDateButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="147dp"
        android:text="Destination"
        app:layout_constraintBottom_toTopOf="@+id/listTipsActivityForAdminForUpdateDestinationSpinner"
        app:layout_constraintEnd_toStartOf="@+id/activityListTripForAdminObjectIdTextUpdateTrip"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listTripsForAdminForUpdateFromSpinner" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="184dp"
        android:layout_marginBottom="2dp"
        android:text="From"
        app:layout_constraintBottom_toTopOf="@+id/listTripsForAdminForUpdateFromSpinner"
        app:layout_constraintEnd_toStartOf="@+id/listTripForAdminFroUpdateTripDepartureDateButton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listViewAdminUpdateTrip" />

    <Button
        android:id="@+id/listTripForAdminFroUpdateTripDepartureDateButton"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_marginEnd="9dp"
        android:onClick="goCalender"
        android:text="Departure Date"
        app:layout_constraintBottom_toTopOf="@+id/activityListTripForAdminObjectIdTextUpdateTrip"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/listTripsForAdminForUpdateFromSpinner"
        app:layout_constraintTop_toBottomOf="@+id/listTripActivityForAdminForUpdateTripDateText" />

    <TextView
        android:id="@+id/listTripActivityForAdminForUpdateTripDateText"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:layout_marginTop="352dp"
        android:layout_marginBottom="7dp"
        android:text="Selected Date Time"
        app:layout_constraintBottom_toTopOf="@+id/listTripForAdminFroUpdateTripDepartureDateButton"
        app:layout_constraintEnd_toEndOf="@+id/listTripForAdminFroUpdateTripDepartureDateButton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/listTripForAdminFroUpdateTripDepartureDateButton"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>